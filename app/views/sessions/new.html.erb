<h1>Sign In!</h1>

<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
    
    <p>
    <%= label_tag(:name) %>
    <%= text_field_tag(:name)%>
    </p>

    <p>
    <%= label_tag(:password) %>
    <%= text_field_tag(:password)%>
    </p>

     <p>
    <%= submit_tag("Sign In")%>
    </p>

<% end %>

